{
    "languageserver": {
        "golang": {
            "command": "gopls",
            "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
            "filetypes": ["go"],
            "initializationOptions": {
                "usePlaceholders": true
            }
        }
    },
    "git.enableGutters": false,
    "tsserver.log": "verbose",
    "tsserver.npm":"/usr/sbin/npm",
    "coc.preferences.enableFloatHighlight": true
}
